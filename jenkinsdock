pipeline {
       agent any
       stages {
            stage("test"){
                steps {
                   git branch: master , credentialsId: 'yashwanthvr3', url: 'https:https://github.com/yashwanthvr3/myproject.git'
                }
            }  
            stage("build"){               
                steps {      
                   sh 'docker build -t  primenumber ./myproject-new/.'
                }
            }  
            stage("deployment"){  
                steps {  
                   sh 'docker run -d -p 6000:5000 --name test new1'
                }
            }
       }
}  
